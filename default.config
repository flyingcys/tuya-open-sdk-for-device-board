
#
# config of linux-ubuntu-6.2.0_64Bit
#

#
# board
#
CONFIG_OPERATING_SYSTEM=100
CONFIG_ENABLE_WIRED=y
CONFIG_LITTLE_END=1
CONFIG_WLAN_DEV="wlan0"
CONFIG_WLAN_AP="wlan1"
# CONFIG_NL80211 is not set
CONFIG_WIFI_DB_PATH="./tuya_db_files"
# end of board

# CONFIG_ENABLE_LIBLWIP is not set

#
# mbedtls Config
#
# CONFIG_ENABLE_BLACK_WHITE_LIST is not set
CONFIG_MBEDTLS_CONFIG_FILE="tuya_tls_config.h"
CONFIG_MAX_SECURITY_LEVEL=1
CONFIG_TUYA_SECURITY_LEVEL=1
# CONFIG_ENABLE_MBEDTLS_DEBUG is not set
# CONFIG_ENABLE_CUSTOM_CONFIG is not set
# end of mbedtls Config

#
# System function Config
#
CONFIG_ENABLE_LOG=y
CONFIG_STACK_SIZE_TIMERQ=4096
CONFIG_STACK_SIZE_WORK_QUEUE=5120
CONFIG_MAX_NODE_NUM_WORK_QUEUE=100
CONFIG_STACK_SIZE_MSG_QUEUE=4096
CONFIG_MAX_NODE_NUM_MSG_QUEUE=100
# end of System function Config

